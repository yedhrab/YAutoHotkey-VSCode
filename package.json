{
	"name": "yautohotkey",
	"displayName": "âœ¨ YAutoHotkey",
	"description": "Detailed AutoHotkey language support and syntax highlighter",
	"version": "1.1.1",
	"publisher": "yedhrab",
	"engines": {
		"vscode": "^1.35.0"
	},
	"icon": "res/yahk.png",
	"license": "MIT",
	"categories": [
		"Programming Languages",
		"Snippets"
	],
	"keywords": [
		"autohotkey",
		"autohotkey+",
		"autohotkey plus",
		"ahk",
		"ahk+",
		"yahk",
		"yautohotkey"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/yedhrab/YAutoHotkey-VsCode"
	},
	"contributes": {
		"languages": [
			{
				"id": "ahk",
				"aliases": [
					"AutoHotkey",
					"autohotkey",
					"ahk"
				],
				"extensions": [
					".ahk"
				],
				"configuration": "./ahk.configuration.json"
			}
		],
		"grammars": [
			{
				"language": "ahk",
				"scopeName": "source.ahk",
				"path": "./syntaxes/ahk.tmLanguage"
			}
		],
		"snippets": [
			{
				"language": "ahk",
				"path": "./snippets/ahk.json"
			}
		]
	}
}
